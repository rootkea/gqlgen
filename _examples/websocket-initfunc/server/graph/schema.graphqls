# GraphQL schema example
#

type Dummy {
  id: ID!
  text: String!
  done: Boolean!
}

type Mutation {
  postMessageTo(subscriber: String!, content: String!): ID!
  postAnnouncement(content: String!): String!
}

type Subscription {
  subscribe(subscriber: String!): String!
  announcement(subscriber: String!): String!
}

type Query {
  foo(id: ID!): String!
}
